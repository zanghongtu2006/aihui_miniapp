
.mix-load-more {
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
	height: 90rpx;
}
.mix-load-icon {
	display: block;
	width: 36rpx;
	height: 36rpx;
	margin-right: 12rpx;
	-webkit-animation: load 1.2s cubic-bezier(0.37, 1.08, 0.7, 0.74) infinite;
	        animation: load 1.2s cubic-bezier(0.37, 1.08, 0.7, 0.74) infinite;
}
.mix-load-text {
	font-size: 28rpx;
	color: #888;
}
@-webkit-keyframes load {
0% {
		-webkit-transform: rotate(0deg);
		        transform: rotate(0deg);
}
100% {
		-webkit-transform: rotate(360deg);
		        transform: rotate(360deg);
}
}

