
.login_img {
	width: 120rpx;
	height: 120rpx;
	border-radius: 10rpx;
	display: block;
	margin: 0 auto;
	margin-top: 120rpx;
	margin-bottom: 40rpx;
}
.login_line {
	width: 94%;
	margin: 60rpx auto;
	border-bottom: 1px solid #eeeeee;
	display: flex;
	align-items: center;
	height: 60rpx;
}
.login_line_title {
	font-size: 28rpx;
	color: #333;
	width: 130rpx;
}
.login_line_input {
	width: 50%;
	font-size: 26rpx;
}
.login_line_ver {
	font-size: 26rpx;
	color: #333;
	border: 1px solid #333;
	padding: 2rpx 5rpx;
	border-radius: 6rpx;
}
.login_btn {
	width: 94%;
	height: 80rpx;
	text-align: center;
	background: #fe7e97;
	color: #ffffff;
	margin: 100rpx auto;
	border-radius: 10rpx;
	line-height: 80rpx;
}

